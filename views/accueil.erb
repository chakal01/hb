<h2>Bienvenue !</h2>

<div class="gallery" style="text-align: center; background-color: <%= @bgColor%>;">
    <div style="width: 100%">
      <img src="<%= @imgAccueil %>" style="max-width: 700px"/>
      <div class="map">
        <p>OÃ¹ me trouver ?</p>
        <div id="map"></div>
      </div>
    </div>
</div>


<script>
  function initMap() {
    var gps_coords = {lat: 47.294594, lng: 5.101444};
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 13,
      center: gps_coords
    });
    var marker = new google.maps.Marker({
      position: gps_coords,
      map: map
    });
  }
</script>
<script async defer
    src="https://maps.googleapis.com/maps/api/js?key=<%=@gmap_key %>&callback=initMap">
</script>
